{
  "name": "server",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "dev": "nodemon --quiet src/index.ts",
    "debug": "nodemon --exec 'node --inspect -r tsconfig-paths/register -r ts-node/register src/index.ts'",
    "exec": "node --experimental-specifier-resolution=node --loader ts-node/esm",
    "start": "yarn run exec .",
    "start:node": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "build": "tsc",
    "db:drop": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "yarn run exec prisma/seed.ts",
    "db:update": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "fastify-plugin": "^4.5.0",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
